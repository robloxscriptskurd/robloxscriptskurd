local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

OrionLib:MakeNotification({
	Name = "Kurd scripts",
	Content = "Thankyou For Using Kurd scripts",
	Image = "rbxassetid://16150566282",
	Time = 5
})

local Window = OrionLib:MakeWindow({Name = "kurd scripts | Made by ahmad", HidePremium = false, SaveConfig = false, ConfigFolder = "Kurd scripts"})

OrionLib:MakeNotification({
	Name = "Kurd Scripts",
	Content = "Thankyou For Using kurd scripts | made by ahmad",
	Image = "rbxassetid://16150566282",
	Time = 5
})

local Tab = Window:MakeTab({
	Name = "Player Tools",
	Icon = "rbxassetid://16152568433",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Unload GUI",
	Callback = function()
      		OrionLib:Destroy()
  	end    
})

